#include <iostream>
#include <cmath>
using namespace std;


int main() {

    // Find square root of 'n' entered by the user.
    cout << "Program starts -->" << endl << endl;

    float n;

    cout << "Enter n: ";
    cin >> n;

    float result = sqrt(n);
    cout << "Result: " << result << endl << endl;

    cout << "<-- Program ends" << endl;

    return 0;
}
